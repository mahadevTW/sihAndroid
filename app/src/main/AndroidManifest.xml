<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bhakti.electricityapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Registration" />
        <activity android:name=".Login" />
        <activity android:name=".NewConnection" />
        <activity android:name=".UserHome" />
        <activity android:name=".ViewBill" />
        <activity android:name=".History" />
        <activity android:name=".Complaint" />
        <activity android:name=".Notifications" />
        <activity android:name=".QuickPay" />
        <activity android:name=".PayOthers" />
        <activity android:name=".ReaderActivity" />
        <activity android:name=".MyComplaint"></activity>
    </application>

</manifest>